{
	"compilerOptions": {
		"baseUrl": ".",
		"outDir": "dist",
		"sourceMap": true,
		"target": "es5",
		"module": "ESNext",
		"moduleResolution": "node",
		"newLine": "LF",
		"strict": true,
		"allowJs": true,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"noUnusedLocals": true,
		"experimentalDecorators": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"removeComments": false,
		"jsx": "preserve",
		"lib": ["esnext", "dom", "dom.iterable"]
	}
}
